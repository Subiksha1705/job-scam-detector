generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Analysis {
  id             String   @id @default(uuid())
  rawText        String
  sourcePlatform String?
  riskLevel      String
  riskScore      Int
  reasons        String
  modelUsed      String
  createdAt      DateTime @default(now())
}
